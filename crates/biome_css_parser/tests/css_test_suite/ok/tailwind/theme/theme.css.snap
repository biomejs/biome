/* @theme directive */
@theme {
  --color-primary: #3b82f6;
  --color-secondary: #10b981;
  --spacing-base: 1rem;
  --font-size-lg: 1.125rem;
}

/* Empty @theme */
@theme {
}

/* @theme with complex values */
