div {
	/* basic usage */
	--basic-1: attr(data-count);
	--basic-2: attr(href);

	/* with type */
	--px-type: attr(data-width px);
	--rem-type: attr(data-size rem);
	--raw-string-type: attr(data-name raw-string);
	--custom-ident-type: attr(id type(<custom-ident>));
