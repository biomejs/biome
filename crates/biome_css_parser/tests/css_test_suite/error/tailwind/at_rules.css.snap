```css
/* Tailwind CSS 4.0 At-Rules Test Cases */

/* @theme directive */
@theme {
  --color-primary: #3b82f6;
  --color-secondary: #10b981;
  --spacing-base: 1rem;
}

/* @utility directive */
@utility tab-4 {
  tab-size: 4;
}

@utility center-flex {
  display: flex;
  align-items: center;
  justify-content: center;
}

/* @variant directive */
@variant dark {
  background: black;
  color: white;
}

@variant mobile {
  @media (max-width: 768px) {
    display: block;
  }
}

/* @custom-variant directive */
@custom-variant theme-midnight (&:where([data-theme="midnight"] *));

@custom-variant hover-focus "(&:hover, &:focus)";

/* @apply directive */
@apply text-lg font-bold;

@apply flex items-center justify-between px-4 py-2;

/* @source directive */
@source "../node_modules/@my-company/ui-lib";

@source "./components/shared.css";

/* @reference directive */
@reference "../../app.css";

@reference "./base-styles.css";

/* @config directive (compatibility) */
@config "../../tailwind.config.js";

/* @plugin directive (compatibility) */
@plugin "@tailwindcss/typography";

@plugin "./custom-plugin.js";

