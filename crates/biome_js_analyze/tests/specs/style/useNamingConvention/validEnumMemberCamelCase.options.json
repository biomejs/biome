{
	"$schema": "../../../../../../packages/@biomejs/biome/configuration_schema.json",
	"linter": {
		"rules": {
			"style": {
				"useNamingConvention": {
					"level": "error",
					"options": {
						"strictCase": true,
						"conventions": [{
							"selector": { "kind": "enumMember" },
							"formats": ["camelCase"]
						}]
					}
				}
			}
		}
	}
}
