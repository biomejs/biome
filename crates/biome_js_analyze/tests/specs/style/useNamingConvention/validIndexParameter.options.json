{
	"linter": {
		"rules": {
			"style": {
				"useNamingConvention": {
					"level": "error",
					"options": {
						"conventions": [
							{
								"selector": {
									"kind": "typeParameter"
								},
								"formats": ["PascalCase"]
							},
							{
								"selector": {
									"kind": "indexParameter"
								},
								"formats": ["camelCase"]
							}
						]
					}
				}
			}
		}
	}
}
