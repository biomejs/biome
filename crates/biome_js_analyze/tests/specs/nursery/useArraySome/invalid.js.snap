cond ? (items.find(x => x > 1) ? "a" : "b") : "c";

items.find(x => x > 1) !== undefined;
items.findLast(x => x > 1) !== undefined;

items.find(x => x > 1) != null;
items.findLast(x => x > 1) != null;

