{
	"$schema": "../../../../../../packages/@biomejs/biome/configuration_schema.json",
	"linter": {
		"enabled": true,
		"rules": {
			"nursery": {
				"useImportExtensions": {
					"level": "error",
					"options": {
                      "importMappings": [
                        {
                          "fileExt": ["ts"],
                          "importExt": "js",
                          "componentImportExt": "jsx"
                        }
                      ]
                    }
				}
			}
		}
	}
}
