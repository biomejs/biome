        "noUselessFragments": {
          "anyOf": [
            {
              "$ref": "#/definitions/NoUselessFragmentsConfiguration"
            },
            {
              "type": "null"
            }
          ],
          "description": "Disallow unnecessary fragments"
        },
        "noUselessLabel": {
          "anyOf": [
            {
              "$ref": "#/definitions/NoUselessLabelConfiguration"
            },
            {
              "type": "null"
            }
          ],
