        },
        "noCommaOperator": {
          "anyOf": [
            {
              "$ref": "#/definitions/NoCommaOperatorConfiguration"
            },
            {
              "type": "null"
            }
          ],
          "description": "Disallow comma operator."
        },
        "noEmptyTypeParameters": {
          "anyOf": [
            {
              "$ref": "#/definitions/NoEmptyTypeParametersConfiguration"
            },
            {
              "type": "null"
            }
          ],
