{
	"name": "@biomejs/website",
	"private": true,
	"scripts": {
		"start": "astro dev",
		"format": "cargo biome-cli-dev format --write .",
		"check": "cargo biome-cli-dev check --apply .",
		"tsc": "tsc --skipLibCheck",
		"start:playground": "pnpm build:wasm-dev && pnpm start",
		"build": "pnpm build:wasm && pnpm build:js",
		"build:js": "astro build",
		"build:wasm": "wasm-pack build --out-dir ../../packages/@biomejs/wasm-web --target web --release --scope biomedev ../crates/biome_wasm",
		"build:wasm-dev": "wasm-pack build --out-dir ../../packages/@biomejs/wasm-web --target web --scope biomedev ../crates/biome_wasm",
		"preview": "astro preview",
		"textlint": "textlint './src/content/docs/ja/**/*.{md,mdx}'",
		"textlint:fix": "textlint --fix './src/content/docs/ja/**/*.{md,mdx}'"
	},
	"devDependencies": {
		"@astrojs/netlify": "^5.1.3",
		"@astrojs/prism": "^3.0.0",
		"@astrojs/react": "^3.1.0",
		"@astrojs/rss": "^4.0.5",
		"@astrojs/starlight": "^0.21.1",
		"@biomejs/wasm-web": "link:../packages/@biomejs/wasm-web",
		"@codemirror/lang-javascript": "^6.2.2",
		"@codemirror/lang-json": "^6.0.1",
		"@codemirror/lint": "^6.5.0",
		"@codemirror/state": "^6.4.1",
		"@codemirror/view": "^6.26.0",
		"@docsearch/css": "^3.6.0",
		"@docsearch/js": "^3.6.0",
		"@fontsource/inter": "^5.0.17",
		"@types/node": "^20.11.27",
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@uiw/react-codemirror": "^4.21.24",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.18",
		"codemirror-lang-rome-ast": "^0.0.6",
		"fast-diff": "^1.3.0",
		"hast-util-to-html": "^9.0.0",
		"hast-util-to-string": "^3.0.0",
		"hastscript": "^9.0.0",
		"html-escaper": "^3.0.3",
		"lang-rome-formatter-ir": "0.0.2",
		"mdast-util-to-hast": "^13.1.0",
		"mermaid": "^10.9.0",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"remark-toc": "^9.0.0",
		"sass": "^1.72.0",
		"sharp": "^0.33.2",
		"textlint": "^14.0.4",
		"textlint-rule-preset-jtf-style": "^2.3.14",
		"textlint-rule-prh": "^6.0.0",
		"typescript": "^5.4.2",
		"vite": "^5.1.6",
		"vite-plugin-svgr": "^4.2.0"
	},
	"dependencies": {
		"astro": "^4.5.3",
		"astro-og-canvas": "^0.4.2",
		"canvaskit-wasm": "^0.39.1"
	}
}
